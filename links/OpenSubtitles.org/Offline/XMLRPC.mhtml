), 'cd1' =3D&gt; array( 'subhash' =3D&gt; $md5subhash, 'subfilename' =3D&gt=
; $subfilename, 'moviehash' =3D&gt; $moviehash, 'moviebytesize' =3D&gt; $mo=
viebytesize, 'movietimems' =3D&gt; $movietimems, 'moviefps' =3D&gt; $movief=
ps, 'movieframes' =3D&gt; $movieframes, 'moviefilename' =3D&gt; $moviefilen=
ame, 'subcontent' =3D&gt; $subtitlecontent ), 'cd2' =3D&gt; array (...) ) )=
