{
  "name": "SubPlayer",
  "manifest_version": 2,
  "version": "0.01",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  //   "options_page": "options.html",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "lib/md5.min.js",
        "lib/jquery-3.2.1.min.js",
        "contentscript/contentscript.js"
      ],
      "css": [
        "contentscript/contentscript.css"
      ]
    },{
      "run_at": "document_end",
      "matches": [
        "*://*.netflix.com/*"
      ],
      "all_frames": true,
      "js": [
        "lib/jquery-3.2.1.min.js",
        "contentscript/netflix.js"
      ]
    },{
      "run_at": "document_end",
      "matches": [
        "*://*.youtube.com/*"
      ],
      "all_frames": true,
      "js": [
        "lib/jquery-3.2.1.min.js",
        "contentscript/youtube.js"
      ]
    }
  ],
  "browser_action": {
    //     "default_icon": {"38": "to_38.png", "19": "to_19.png"}
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
  // "web_accessible_resources": [
  //   "contentscript/contentscript.css"
  // ]
  //     "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com https://translate.google.com; object-src 'self'"
}